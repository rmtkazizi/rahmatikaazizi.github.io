        "basket['hari'] = basket['basket_date'].dt.dayofweek\n",
        "basket['weekend'] = 0\n",
        "baskets= (basket['hari']==5) | (basket['hari']==6)\n",
        "basket['weekend'] = 0\n",
        "basket.loc[baskets, 'weekend'] = 1\n",
        "basket"
      ]
    },
